<?xml version="1.0" encoding="UTF-8" ?>
<lcui-app>
  <resource type="text/css" src="assets/stylesheets/lcui.css"/>
  <resource type="text/css" src="assets/stylesheets/main.css"/>
  <resource type="text/css" src="assets/stylesheets/themes/green.css"/>
  <ui>
    <w id="main-window" class="window theme-green">
      <resource type="text/xml" src="assets/views/sidebar.xml"/>
      <resource type="text/xml" src="assets/views/search.xml"/>
      <resource type="text/xml" src="assets/views/home.xml"/>
      <resource type="text/xml" src="assets/views/folders.xml"/>
      <resource type="text/xml" src="assets/views/settings.xml"/>
      <w class="app-alert">
        <w class="alert alert-primary hide">
          <w id="file-sync-tip-title" type="textview-i18n" class="alert-header text" data-i18n-key="filesync.title.syncing">正在同步你的资源</w>
          <w id="file-sync-tip-stats" type="textview-i18n" class="text alert-text" data-i18n-key="filesync.text.scaning">已扫描 0 个文件</w>
        </w>
      </w>
    </w>
  </ui>
</lcui-app>
